{
	"hosting":{
		"public":"dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"headers": [ {
		  "source": "**/*.@(jpg|jpeg|gif|png|pdf)",
		  "headers": [ {
			"key": "Cache-Control",
			"value": "max-age=7200"
		  } ]
		}],
		"rewrites":[
			{
				"source":"**",
				"destination":"/index.html"
			}
		]

	}
}
